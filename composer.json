{
	"name": "natanfelles/app",
	"type": "project",
	"description": "The App project",
	"keywords": [
		"app",
		"application"
	],
	"homepage": "https://the-framework.gitlab.io/packages/app.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0",
		"natanfelles/mvc": "v3.0.0-alpha.1"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.13",
		"natanfelles/coding-standard": "^1.2",
		"natanfelles/php-server": "^2.11",
		"phpmd/phpmd": "^2.10",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
			"App.php"
		],
		"files": [
			"config/constants.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-update-cmd": [
			"composer dump-autoload --optimize"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/packages/app/issues",
		"wiki": "https://gitlab.com/the-framework/packages/app/wikis/home",
		"source": "https://gitlab.com/the-framework/packages/app",
		"docs": "https://the-framework.gitlab.io/packages/app/docs/"
	}
}
